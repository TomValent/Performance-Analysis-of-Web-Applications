{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "types": ["@opentelemetry/api", "@opentelemetry/metrics"],
    "alwaysStrict": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "sourceMap": true,
    "noImplicitAny": true,
    "strictFunctionTypes": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
